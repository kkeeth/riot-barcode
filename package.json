{
  "description": "riot-barcode is a simple barcode generator for riotjs by using JsBarcode library.",
  "version": "0.0.4",
  "author": "kkeeth <zensin0082@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkeeth/riot-barcode/issues"
  },
  "main": "bin/riot-barcode.min.js",
  "homepage": "https://github.com/kkeeth/riot-barcode#readme",
  "keywords": [
    "riot",
    "riotjs",
    "barcode",
    "riot-barcode"
  ],
  "scripts": {
    "cov-html": "nyc report --reporter=html",
    "test": "nyc --require esm --require jsdom-global/register --require ./test/register-riot-extension.js mocha src/**/*.spec.js",
    "clean": "rimraf ./coverage && rimraf ./bin",
    "start": "npm run clean && npm run watch",
    "compile": "riot src/riot-barcode.riot --output bin/component/riot-barcode.js",
    "compile-watch": "riot -w src/riot-barcode.riot --output bin/component/riot-barcode.js",
    "build": "npm run clean && webpack --mode development && webpack --mode production",
    "watch": "webpack -w --mode development && cp:js & webpack-dev-server --hot --inline -d",
    "cp:js": "cp bin/riot-barcode.js example",
    "cp:css": "cp assets/css/style.css example",
    "before": "npm run cp:js && npm run cp:css",
    "deploy": "npm run before && gh-pages -d example"
  },
  "dependencies": {
    "jsbarcode": "^3.11.0",
    "riot": "^4.13.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@riotjs/cli": "^4.0.3",
    "@riotjs/compiler": "4.8.2",
    "@riotjs/hot-reload": "^4.0.1",
    "@riotjs/ssr": "^4.2.1",
    "@riotjs/webpack-loader": "^4.0.3",
    "babel-loader": "^8.1.0",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "esm": "^3.2.25",
    "gh-pages": "^3.1.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.1.1",
    "nyc": "^15.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-date-fns-externals": "^1.0.2",
    "webpack-dev-server": "^3.11.0"
  },
  "name": "riot-barcode",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kkeeth/riot-barcode.git"
  }
}
